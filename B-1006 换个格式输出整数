#include <iostream>
#include<cstdio>
using namespace std;

int main()
{
    int n;
    scanf("%d",&n);
    int a,b,c;
    a=n/100;
    n=n%100;
    b=n/10;
    n=n%10;
    c=n;
    for(int i=0;i<a;i++)
    {
        printf("B");
    }
    for(int i=0;i<b;i++)
    {
        printf("S");
    }
    if(c!=0)
    {
        for(int i=1;i<=c;i++)
        {
            printf("%d",i);
        }
    }
}
